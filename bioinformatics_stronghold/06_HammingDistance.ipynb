    "file = \"sample.txt\"\n",
    "\n",
    "with open(file, \"r\") as f:\n",
    "    lines = f.read().splitlines()\n",
    "    dna1 = lines[0]\n",
    "    dna2 = lines[1]\n",
    "    \n",
    "    hamming_distance = sum(a != b for a, b in zip(dna1, dna2))\n",
    "\n",
    "    print(hamming_distance)"
